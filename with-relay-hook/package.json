{
  "name": "with-relay-hook",
  "version": "1.0.0",
  "description": "Example of Next.js with Relay Hook",
  "scripts": {
    "graphcool-init": "graphcool init --schema schema/init-schema.graphql",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "relay": "relay-compiler --src ./ --schema ./schema/schema.graphql --language typescript --artifactDirectory ./__relay_artifacts__ --exclude 'schema/**' '**/.next/**' '**/node_modules/**' '**/test/**'  '**/__relay_artifacts/**' '**/server/**'",
    "schema": "graphql get-schema -e dev"
  },
  "license": "ISC",
  "dependencies": {
    "graphql": "15.3.0",
    "next": "latest",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-relay": "9.1.0",
    "relay-hooks": "3.5.1",
    "relay-runtime": "9.1.0"
  },
  "devDependencies": {
    "@types/next": "9.0.0",
    "@types/react-relay": "7.0.8",
    "@types/relay-runtime": "9.1.5",
    "babel-plugin-relay": "9.1.0",
    "graphcool": "0.11.6",
    "graphql-cli": "4.0.0",
    "relay-compiler": "9.1.0",
    "relay-compiler-language-typescript": "12.0.3",
    "relay-devtools": "1.4.0",
    "typescript": "3.9.6"
  }
}
